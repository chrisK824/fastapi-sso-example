{
    "version": 2,
    "public": false,
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 80",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}