{
    "public": false,
    "buildCommand": null,
    "framework": null,
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 80",
    "functions": {
        "api/*.py": {
          "runtime": "vercel-python@latest"
        }
    },
    "rewrites": [
        {
            "src": "/(.*)",
            "destination": "main.py"
        }
    ]
}
